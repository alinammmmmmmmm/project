{
  "name": "project",
  "description": "Playwright Tests with codeceptJS",
  "version": "1.0.0",
  "main": "index.js",
  "devDependencies": {
    "@playwright/test": "^1.32.3",
    "codeceptjs": "^3.4.1"
  },
  "scripts": {
    "headless": "HEADLESS=true codeceptjs run",
    "ui": "npm run codeceptjs:ui",
    "headed": "npx codeceptjs run",
    "headless-steps": "HEADLESS=true codeceptjs run --steps",
    "headed-by-steps": "npx codeceptjs run --steps",
    "debug": "npx codeceptjs run --debug",
    "verbose": "npx codeceptjs run --verbose",
    "list": "npx codeceptjs list",
    "headed-by-filename": "npx codeceptjs run src/test.spec.js"
  },
  "dependencies": {
    "@codeceptjs/ui": "^0.3.8"
  }
}
